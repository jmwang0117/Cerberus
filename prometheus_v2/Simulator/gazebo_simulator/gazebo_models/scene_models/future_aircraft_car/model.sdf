    <joint type="revolute" name="left_front_wheel_joint">
      <pose>0 0 0 1.57 0 0</pose>
      <child>left_front_wheel</child>
      <parent>base_link</parent>
      <axis>
        <xyz>0 1 0</xyz>
      </axis>
    </joint>

    <joint type="revolute" name="left_back_wheel_joint">
      <pose>0 0 0 1.57 0 0</pose>
      <child>left_back_wheel</child>
      <parent>base_link</parent>
      <axis>
        <xyz>0 1 0</xyz>
      </axis>
    </joint>

    <joint type="revolute" name="right_front_wheel_joint">
      <pose>0 0 0 1.57 0 0</pose>
      <child>right_front_wheel</child>
      <parent>base_link</parent>
      <axis>
        <xyz>0 1 0</xyz>
      </axis>
    </joint>

    <joint type="revolute" name="right_back_wheel_joint">
      <pose>0 0 0 1.57 0 0</pose>
      <child>right_back_wheel</child>
      <parent>base_link</parent>
      <axis>
        <xyz>0 1 0</xyz>
      </axis>
    </joint>

    <joint type="fixed" name="target_pad_fixed">
      <pose>0 0 0 0 0 0</pose>
      <child>target_pad_link</child>
      <parent>base_link</parent>
    </joint>

    <include>
      <uri>model://noisy_imu</uri>
      <pose>0 0 0.1 0 0 0</pose>
    </include>
    <joint type="fixed" name="imu_fixed">
      <child>imu::imu_link</child>
      <parent>base_link</parent>
    </joint>

    <plugin name="imu_plugin" filename="libgazebo_ros_imu.so">
      <alwaysOn>true</alwaysOn>
      <bodyName>imu::imu_link</bodyName>
      <topicName>/imu</topicName>
      <serviceName>imu_service</serviceName>
      <gaussianNoise>0.0</gaussianNoise>
      <updateRate>200.0</updateRate>
    </plugin>

